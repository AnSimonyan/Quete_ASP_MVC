@model List<Quete_ASP_MVC.Models.Item>

@{
    Layout = "_Layout";
    ViewData["Title"] = "Items";
}
<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" href="site.css">
</head>
<body>
<h2>Items</h2>
<ul>
    <Table>
        <tr>
            @foreach (var item in Model)
            {
                <td>@item.Name </td>
            }
        </tr>
    </Table>
</ul>
</body>